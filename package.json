{
  "name": "silverstripe-passkey-auth",
  "version": "1.0.0",
  "description": "Passkey authentication module for SilverStripe",
  "main": "client/dist/js/passkey-auth.js",
  "scripts": {
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "dev": "webpack serve --mode=development --open",
    "clean": "rm -rf client/dist/",
    "lint:js": "eslint client/src/js/**/*.js",
    "lint:scss": "stylelint client/src/scss/**/*.scss",
    "lint": "yarn lint:js && yarn lint:scss",
    "test": "jest",
    "analyze": "webpack-bundle-analyzer client/dist/js/*.js",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "files": [
    "client/dist/",
    "src/",
    "templates/",
    "_config/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.23.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "core-js": "^3.33.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-loader": "^7.3.3",
    "sass": "^1.80.0",
    "sass-loader": "^14.0.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "keywords": [
    "passkey",
    "webauthn",
    "silverstripe",
    "authentication",
    "passwordless",
    "biometric"
